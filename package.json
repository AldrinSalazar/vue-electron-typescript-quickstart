{
  "name": "vue-electron-typescript-quickstart",
  "version": "0.1.4",
  "private": false,
  "description": "A boilerplate of Electron app that uses Vue in TypeScript.",
  "keywords": [
    "Vue.js",
    "Electron",
    "TypeScript",
    "JavaScript",
    "boilerplate",
    "quickstart",
    "template"
  ],
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir -c electron-builder.yml",
    "dist": "electron-builder -c electron-builder.yml",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:mainproc": "webpack-cli --mode=development --config webpack.mainproc.config.js",
    "clean": "rm -rf ./dist && rm -rf ./bin && rm -rf ./dist.electron",
    "lint": "vue-cli-service lint",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/electron": "^1.6.10",
    "@types/mocha": "^5.2.6",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-plugin-unit-mocha": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "chai": "^4.2.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.4.5",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shellyln/vue-electron-typescript-quickstart.git"
  }
}
